{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nebula",
  "font": {
    "headings": {
      "family": "Plus Jakarta Sans"
    },
    "body": {
      "family": "IBM Plex Sans"
    }
  },
  "colors": {
    "primary": "#5040ff",
    "light": "#9373ff",
    "dark": "#2e20cc"
  },
  "favicon": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.png"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart"
            ]
          },
          {
            "group": "Core Guides",
            "pages": [
              "guides/concepts",
              "guides/memory-operations",
              "guides/collections",
              "guides/conversations"
            ]
          },
          {
            "group": "Advanced Guides",
            "pages": [
              "guides/search",
              "guides/metadata-filtering",
              "guides/advanced-features"
            ]
          },
          {
            "group": "Integration",
            "pages": [
              "mcp-integration"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Client Libraries",
            "pages": [
              "clients/python",
              "clients/nodejs"
